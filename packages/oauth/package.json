{
	"name": "@camunda8/oauth",
	"version": "8.6.2",
	"description": "",
	"main": "./dist/index.js",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"test": "xo && ava",
		"build": "npm run clean && npm run write-package-version && tsc --project tsconfig.json",
		"clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
		"lint": "xo",
		"format": "xo --fix",
		"write-package-version": "node ../../write-package-version.js"
	},
	"keywords": [
		"zeebe",
		"zeebe-node",
		"camunda",
		"automation",
		"bpmn",
		"camunda 8",
		"operate",
		"optimize",
		"tasklist",
		"web modeler",
		"oauth"
	],
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"author": "josh.wulf@camunda.com",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/camunda/camunda-8-js-sdk.git"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsx/esm"
		],
		"workerThreads": false
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"xo": {
		"rules": {
			"@typescript-eslint/semi": "off"
		}
	},
	"dependencies": {
		"@camunda8/certificates": "^1.0.0",
		"debug": "^4.3.7",
		"jwt-decode": "^4.0.0",
		"ky": "^1.7.2",
		"neon-env": "^0.2.2"
	},
	"devDependencies": {
		"@types/debug": "^4.1.12",
		"@types/jsonwebtoken": "^9.0.7",
		"ava": "^6.1.3",
		"cz-conventional-changelog": "^3.3.0",
		"delay": "^6.0.0",
		"eslint-config-xo-typescript": "^7.0.0",
		"get-port": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"tsx": "^4.19.1",
		"xo": "^0.59.3"
	}
}