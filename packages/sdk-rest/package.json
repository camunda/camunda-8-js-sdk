{
	"name": "@camunda8/sdk-rest",
	"version": "8.6.2",
	"description": "",
	"main": "distribution/index.js",
	"type": "module",
	"scripts": {
		"test": "xo && npm run build && npm run setup-test-env && ava distribution/test/**/*.js --serial",
		"setup-test-env": "npm run copy-test-resources && npm run clean-test-env",
		"clean-test-env": "node distribution/test/helpers/_clean-test-env.js",
		"copy-test-resources": "cp -r source/test/resources distribution/test",
		"build": "npm run clean && npm run write-package-version && tsc --project tsconfig.json",
		"clean": "rm -rf ./distribution && rm -f ./tsconfig.tsbuildinfo",
		"lint": "xo",
		"format": "xo --fix",
		"write-package-version": "node ../../write-package-version.js"
	},
	"keywords": [
		"zeebe",
		"zeebe-node",
		"camunda",
		"automation",
		"bpmn",
		"camunda 8",
		"operate",
		"optimize",
		"tasklist",
		"web modeler",
		"modeler"
	],
	"private": false,
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"author": "josh.wulf@camunda.com",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/camunda/camunda-8-js-sdk.git"
	},
	"ava": {
		"typescript": {
			"rewritePaths": {
				"source/": "distribution/"
			},
			"compile": "tsc"
		}
	},
	"xo": {
		"rules": {
			"@typescript-eslint/semi": "off"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@camunda8/lossless-json": "workspace:*",
		"@camunda8/oauth": "workspace:*",
		"@types/multiparty": "^4.2.1",
		"debug": "^4.3.7",
		"eventemitter3": "^5.0.1",
		"ky": "^1.7.2",
		"loglevel": "^1.9.2",
		"neon-env": "^0.2.2"
	},
	"devDependencies": {
		"@ava/typescript": "^5.0.0",
		"@types/debug": "^4.1.12",
		"@types/node": "^22.7.5",
		"@types/uuid": "^10.0.0",
		"ava": "^6.1.3",
		"cz-conventional-changelog": "^3.3.0",
		"delay": "^6.0.0",
		"fast-xml-parser": "^4.5.0",
		"multiparty": "^4.2.3",
		"typedoc": "^0.26.8",
		"typedoc-plugin-missing-exports": "^3.0.0",
		"typescript": "^5.6.2",
		"uuid": "^10.0.0",
		"xo": "^0.59.3"
	}
}
