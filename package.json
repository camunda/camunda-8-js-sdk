{
	"name": "c8-sdk",
	"version": "1.0.0",
	"main": "commitlint.config.js",
	"scripts": {
		"test": "pnpm -r run test",
		"sm:start": "docker compose -f docker/docker-compose-multitenancy.yaml -f docker/docker-compose-modeler.yaml up -d",
		"sm:stop": "docker compose -f docker/docker-compose-multitenancy.yaml -f docker/docker-compose-modeler.yaml down",
		"build": "pnpm -r run build",
		"preinstall": "node ./check-pnpm.js",
		"prepare": "husky install"
	},
	"private": true,
	"type": "module",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"xo": {
		"rules": {
			"semicolon": false
		}
	},
	"lint-staged": {
		"**/*.ts": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"engines": {
		"yarn": ">=1.22.0"
	},
	"installConfig": {
		"hoistingLimits": "workspaces"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^18.5.0",
		"commitlint": "^18.5.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.10",
		"prettier": "^3.3.3"
	}
}
