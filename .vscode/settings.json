{
	"conventionalCommits.scopes": [
		"repo",
		"oauth",
		"tasklist",
		"zeebe",
		"github",
		"modeler",
		"operate",
		"camunda8",
		"optimize",
		"lossless-parser",
		"sdk-rest"
	],
	"editor.formatOnSave": true,
	"xo.enable": true,
	"xo.format.enable": true,
